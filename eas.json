{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "development"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "distribution": "internal",
      "env": {
        "ENVIRONMENT": "preview"
      },
      "cache": {
        "disabled": false
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "ENVIRONMENT": "production"
      },
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}